/* component/accont_row/account_row.wxss */
.asset-row-container{
  height: 100rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  border-style: solid;
  border-radius: 15rpx;
  margin-bottom: 20rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;
}
.account-image{
  width: 10%;
  height: 100%;
  margin-left: 30rpx;
}
.account-name{
  width: 25%;
  margin-left: 30rpx;
}
.account-detail{
  width: 35%;
  display: flex;
  justify-content: flex-start;
}
.font-32{
  font-size: 32rpx;
}

.font-23{
  font-size: 23rpx;
}